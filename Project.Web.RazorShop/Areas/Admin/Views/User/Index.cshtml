@{
    ViewData["Title"] = "لیست کابران ";
}
@section styles{
    <partial name="_filterstyles" />
    <partial name="_HelperStyles" />
}
@section scripts{
    <partial name="_filterscripts" />
    <partial name="_HelprScripts" />
    <partial name="_ValidationScriptsPartial" />
    <script src="~/admin/assets/js/Users.js?@(DateTime.Now.Ticks)"></script>

}

@section headertools{

}
<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body table-responsive">
                    <div class="card-title d-flex justify-content-between">
                        <h6>@ViewBag.Title</h6>
                        <div class="custom-control custom-switch custom-checkbox-danger mx-3">
                            <input type="checkbox" class="custom-control-input" id="showdelted" name="delted" onchange="SwitchDeleted()">
                            <label class="custom-control-label" for="showdelted">حذف شده ها</label>
                        </div>
                    </div>
                    <table id="Table" class="table table-hover table-striped table-advance ajax-data-table">
                        <thead>
                            <tr>
                                <th class="all"># <span class="ti-exchange-vertical"></span></th>
                                <th class="all">نام کاربری <span class="ti-exchange-vertical"></span></th>
                                <th class="all">نام<span class="ti-exchange-vertical"></span></th>
                                <th class="all">نام خانوادگی <span class="ti-exchange-vertical"></span></th>
                                <th class="all">شماره تلفن<span class="ti-exchange-vertical"></span></th>
                                <th>تاریخ ثبت</th>
                                <th class="all">اقدامات</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" tabindex="-1" role="dialog" id="usermodal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">اطلاعات کاربرُ</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <i class="ti-close"></i>
                </button>
            </div>
            <div class="modal-body">

            </div>
        </div>
    </div>
</div>